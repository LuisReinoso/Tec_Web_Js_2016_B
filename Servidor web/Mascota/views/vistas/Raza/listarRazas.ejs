<% for(var i=0; i<razas.length; i++){ %>
  <div class="col-sm-6 col-md-3">
    <div class="thumbnail">
      <img src="/images/razas.png" width="auto" alt="imagenraza">
      <div class="caption bg-info">
        <h3><strong>Datos</strong></h3>
        <p>
          <strong>Nombre</strong>: <%=razas[i].nombre %>
        </p>
        <p>
          <a href="/Raza/eliminiarRaza?id=<%=razas[i].id %>" class="btn btn-danger btn-block" role="button">Borrar</a>
          <a href="/editarRaza?id=<%=razas[i].id%>" class="btn btn-primary btn-block" role="button">Editar</a>
        </p>
      </div>
    </div>
  </div>
<%  } %>
