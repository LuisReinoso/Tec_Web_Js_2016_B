<div class="row">
  <div class="col-sm-5">
    <h2>Editar Usuario</h2>
    <form action="/Usuario/editarUsuarioForm" method="post" class="form-horizontal ">
      <div class="form-group">
        <label class="control-label col-sm-3">Nombres</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" placeholder="Ingrese sus Nombres" name="nombres" value="<%= usuarioAEditar.nombres %>">
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3">Apellidos</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" placeholder="Ingrese sus Apellidos" name="apellidos" value="<%= usuarioAEditar.apellidos %>">
        </div>
        <input type="hidden" name="idUsuario" value="<%= usuarioAEditar.id %>">
      </div>

      <button type="submit" class="btn btn-success btn-block">Editar Usuario</button>
    </form>

  </div>
  <div class="col-sm-1"></div>

  <div class="col-sm-5 form-horizontal">
    <h2>Datos Usuario</h2>

    <div class="col-sm-5 control-label">
      <strong>Nombres</strong>
    </div>
    <div class="col-sm-7">
      <p class="form-control-static">
        <%= usuarioAEditar.nombres%>
      </p>
    </div>

    <div class="col-sm-5 control-label">
      <strong>Apellidos</strong>
    </div>
    <div class="col-sm-7">
      <p class="form-control-static">
        <%= usuarioAEditar.apellidos%>
      </p>
    </div>

    <div class="col-sm-5 control-label">
      <strong>Correo</strong>
    </div>
    <div class="col-sm-7">
      <p class="form-control-static">
        <%= usuarioAEditar.correo%>
      </p>
    </div>

  </div>

</div>
</div>
