<% for(var i=0; i<usuarios.length; i++){ %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="/images/usuarios.gif" width="200px" alt="...">
      <div class="caption">
        <h3>Datos</h3>
        <p>
          <strong>Nombres</strong>: <%=usuarios[i].nombres %>
          <br>
          <strong>Apellidos</strong>: <%=usuarios[i].apellidos %>
          <br>
          <strong>Correo</strong>: <%=usuarios[i].correo %>
        </p>
        <p>
          <a href="/Usuario/eliminiarUsuario?id=<%=usuarios[i].id %>" class="btn btn-primary" role="button">Borrar</a>
          <a href="/editarUsuario?id=<%=usuarios[i].id%>" class="btn btn-primary" role="button">Editar</a>
        </p>
      </div>
    </div>
  </div>
<%  } %>
